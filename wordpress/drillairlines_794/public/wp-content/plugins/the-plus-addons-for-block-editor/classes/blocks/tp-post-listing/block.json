        "order": {
            "type": "string",
            "default": "desc"
        },
        "columns": {
            "type": "object",
            "default": {
                "md": 6,
                "sm": 6,
                "xs": 12
            }
        },
        "columnSpace": {
            "type": "object",
            "default": {
                "md": {
                    "top": 15,
                    "right": 15,
                    "bottom": 15,
                    "left": 15
                },
                "unit": "px"
            },
            "style": [
                {
                    "condition": [
                        {
                            "key": "layout",
                            "relation": "!=",
                            "value": "carousel"
