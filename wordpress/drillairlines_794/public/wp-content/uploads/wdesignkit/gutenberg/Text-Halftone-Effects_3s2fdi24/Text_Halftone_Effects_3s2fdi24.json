            "css":".wkit-text-halftone-grid{\r\n    position: relative;\r\n    display: block;\r\n    text-align: center;\r\n}\r\n\r\n .editor-styles-wrapper .wkit-text-halftone-grid a:hover,  .editor-styles-wrapper .wkit-text-halftone-grid a:focus {\r\n    color: #ff5a6e00 !important;\r\n}\r\n\r\n.wkit-text-halftone-effects .wkit-text-halftone-grid a{\r\n    display:inline-block;\r\n    font-size: 5vw;\r\n    font-weight: bold;\r\n    letter-spacing: 3px;\r\n    margin:0;\r\n    position: relative;\r\n    text-decoration: none;\r\n    transition: 0.3s linear;\r\n}\r\n\r\n.wkit-text-halftone-effects .wkit-text-halftone-grid .halftone-color[ href=\"#\"], \r\n.wkit-text-halftone-effects .wkit-text-halftone-grid .halftone-color[ href=\"\"],\r\n.wkit-text-halftone-effects .wkit-text-halftone-grid .halftone-color:not([ href]) {\r\n    pointer-events: none;\r\n    cursor: default;\r\n}\r\n.wkit-halftone-first{\r\n    z-index:1;\r\n    position: relative;\r\n}\r\n.wkit-stroke-yes{\r\n   -webkit-text-stroke-width: 2px;\r\n   -webkit-text-stroke-color: #111827;\r\n    color: transparent;\r\n}\r\n.texture-yes{\r\n    position: relative;\r\n}\r\n\r\n.texture-yes:after{\r\n    content: attr(data-content);\r\n    background:url(\"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAADCAYAAABWKLW/AAAAHElEQVQYV2NUVFT8f//+fUYGBgYGMAEDcA5IBQCKJQVmn76DhAAAAABJRU5ErkJggg==\");\r\n    --halftoneleftOffset:6px;\r\n    left: calc(50% + var(--halftoneleftOffset));\r\n    position: absolute;\r\n    --halftonetopOffset:6px;\r\n    top: calc(50% + var(--halftonetopOffset));\r\n    transform: translate(-50%, -50%);\r\n    -webkit-text-stroke-width: 0;\r\n    -webkit-background-clip: text;\r\n    background-clip: text;\r\n    -webkit-text-fill-color: transparent;\r\n}\r\n.texture-yes:after{\r\n    background-color: #db2777;\r\n}\r\n.wkit-shadow-inner{\r\n    display:none;\r\n}\r\n.wkit-shadow-inner.layer-yes{\r\n    position:absolute;\r\n    display:block;\r\n    --halftoneleftOffset:6px;\r\n    --layerToffset:1px;\r\n    --layerLoffset:1px;\r\n    left: calc(50% + var(--halftoneleftOffset) + var(--layerLoffset));\r\n    --halftonetopOffset:6px;\r\n    top: calc(50% + var(--halftonetopOffset) + var(--layerToffset));\r\n    transform: translate(-50%, -50%);\r\n    -webkit-text-stroke-width: 0;\r\n    background-clip: text;\r\n    -webkit-text-fill-color: transparent;\r\n    z-index:0;\r\n}\r\n.wkit-shadow-inner.layer-yes.gradient-bg-yes{\r\n    background-clip: text !important;\r\n    -webkit-text-fill-color: transparent !important;\r\n    background: linear-gradient(235.7deg, #231557 -8.86%, #44107A 23.4%, #FF1361 67.18%, #FFF800 103.96%),\r\n    linear-gradient(135deg, #EC2F2F 14.73%, #DD2FEC 100%);\r\n}\r\n    \r\n    \r\n",
