{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "name": "tpgb/tp-post-meta",
    "version": "0.1.0",
    "title": "Post Meta Info",
    "category": "tpgb",
    "description": "Post's Categories, Tags and other meta info.",
    "example": {
        "attributes": {}
    },
	"supports": {
		"align": ["wide","full"]
	},
	"textdomain": "the-plus-addons-for-block-editor",
