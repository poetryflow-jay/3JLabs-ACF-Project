            "type": "string",
            "default": ""
        },
        "metaLayout": {
            "type": "string",
            "default": "layout-1"
        },
        "metaSort": {
            "type": "object",
            "default": {
                "sort": ["Date", "Category", "Author", "Comments", "Post Reading Time"]
            }
        },
        "metafieldRep": {
            "type": "array",
            "repeaterField": [
                {
                    "metaLabel": {
                        "type": "string",
                        "default": ""
                    },
                    "metaDynamic": {
                        "type": "string",
                        "default": ""
                    },
                    "metapostfix": {
                        "type": "string",
                        "default": ""
                    }
                }
            ],
            "default": [
                {
                    "metaLabel": "",
                    "metaDynamic": "",
                    "metapostfix": ""
                }
            ]
        },
        "alignment": {
            "type": "object",
            "default": {
                "md": "left"
            },
            "style": [
                {
                    "selector": "{{PLUS_WRAP}} .tpgb-meta-info,{{PLUS_WRAP}}.tpgb-post-meta {justify-content: {{alignment}};}"
                }
            ],
            "scopy": true
        },
        "metaTypo": {
            "type": "object",
            "default": {
                "openTypography": 0,
                "size": {
                    "md": "",
                    "unit": "px"
