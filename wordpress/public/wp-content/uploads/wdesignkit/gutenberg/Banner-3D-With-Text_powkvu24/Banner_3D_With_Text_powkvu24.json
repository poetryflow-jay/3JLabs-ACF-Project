            "css":".wkit-banner-3d-with-text{\n    display:block;\n    position:relative;\n} \n.banner-3d-inner{\n    transform-style:preserve-3D;    \n    perspective: 1200px;\n}\n.banner-3d-inner-media{ \n    max-width: 100%;\n    margin: 0 auto;\n    z-index:-1;\n    position:relative;\n    overflow: hidden;\n    transform: translateZ(0);\n}\n.banner-3d-inner-media-inner{\n    width:100%;\n    top:0;\n    left:0;\n    height:500px;\n    transition: transform 1s cubic-bezier(0.23, 1, 0.32, 1);\n}\n.banner-3d-inner-media-inner-image{\n    width:100%;\n    height:100%;\n    background-size:cover;\n    background-position:center;\n    margin:0;\n}\n.banner-3d-inner-heading{\n    position: absolute;\n    /*top: 50%;*/\n    /*left: 15px;*/\n    /*right: 40%;*/\n    transform-style: preserve-3d;\n    /*transform: translate3d(0, -50%, 150px) scale(0.875);*/\n}\n.banner-3d-inner-heading-inner{\n   margin: 0;\n   line-height: 1;\n}\n.banner-3d-inner-heading-inner-text{\n    font-weight:700;\n    font-size:90px;\n    color: #000;\n    margin:5px auto;\n    text-align:left;\n    position:relative;\n}\n.banner-3d-inner-heading-inner .banner-3d-inner-heading-content{\n    position: relative;\n    display:flex;\n    align-items: center;\n    gap:10px;\n}\n.banner-3d-inner-heading .tp-title-icon{\n    font-size:18px;\n    color: #000;\n}\n.banner-3d-inner-heading .tp-title-icon .dashicon{\n    width: auto;\n    height: auto;\n}\n.banner-3d-inner-heading-subtitle{\n    height:max-content;\n}\n.banner-3d-inner-heading-subtitle1{\n    font-weight:700;\n    font-size:40px;\n    text-align:left;\n}\n.banner-3d-inner-btn{\n    position: absolute;\n    bottom: 5%;\n    right: 9%;\n    opacity: 1;\n    /*transform: translate3d(0, 0, 100px) scale(0.917);*/\n    transition: all 0.65s cubic-bezier(0.165, 0.84, 0.44, 1);\n}\n.banner-3d-inner-btn-text{\n    margin-top:10px;\n    color: #000;\n    width:max-content;\n    transition:0.3s linear;\n}\n.banner-3d-inner-btn .tp-title-icon{\n    display:inline-flex;\n    width:50px;\n    height:50px;\n    border:1px solid black;\n    border-radius:50%;\n    color: #000;\n    transition:0.3s linear;\n    justify-content:center;\n    align-items:center;\n}\n.banner-3d-inner-btn .tp-title-icon .dashicon{\n    width: auto;\n    height: auto;\n    transition:0.3s linear;\n}\n.btn-banner{\n    display:flex;\n    flex-direction:column;\n    align-items:center;\n    text-decoration: none;\n}\n\n",
