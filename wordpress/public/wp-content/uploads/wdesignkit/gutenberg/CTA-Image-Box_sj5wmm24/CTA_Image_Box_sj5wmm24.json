        "Editor_data": {
            "html":"<div class=\"wkit-cta-image-box\">\n    <div class=\"cta-image-inner\">\n        <img class=\"cta-img\" src=\"{{media_g1sl5u23}}\"></img>\n        <div class=\"cta-content\">\n            <span class=\"cta-content-title\">{{text_8wm4ah23}}</span>\n            <p class=\"cta-content-desc\">{{textarea_90fwfc23}}</p>\n        </div> \n    </div>\n</div>\n",
            "css":".wkit-cta-image-box{\n    display:block;\n    position:relative;\n}\n[class*=\"wkit-wb-Widget_\"] {\n  width: 100%;\n}\n.cta-image-inner{\n    display:flex;\n    position:relative;\n    justify-content:left;\n    align-items:flex-end;\n    padding:20px;\n    height:300px;\n    overflow:hidden;\n    transition: all 0.3s linear;\n}\n.cta-image-inner:hover{\n    /*background-color: #1274E7;*/\n}\n.cta-image-inner:hover .cta-content-title, .cta-image-inner:hover .cta-content-desc{\n    opacity:1;\n}\n.cta-image-inner:hover .cta-content-title{\n    transform:translateY(0%);\n}\n.cta-image-inner img[src=\"\"] {\n   display: none;\n}\n.cta-image-inner .cta-img{\n   position:absolute;\n   width:100%;\n   height:100%;\n   transition:0.3s linear;\n   object-fit:cover;\n   top:0;\n   left:0;\n}\n.cta-image-inner:hover .cta-img{\n    opacity:0.3;\n    transform:scale(1.02);\n}\n.cta-content{\n    display:inline-flex;\n    flex-direction:column;\n    position:relative;\n    width: 100%;\n}\n.cta-content-title{\n    font-size:25px;\n    color:#000;\n    opacity:0;\n    transition: all 0.4s linear;\n    transform:translateY(100%);\n    margin-bottom:10px;\n    \n}\n.cta-content-desc{\n    font-size:15px;\n    margin:0;\n    transition: all 0.3s linear;\n    opacity:0;\n    color:#000;\n}\n@media (min-width: 768px) and (max-width: 1024px){\n    .cta-content-title,.cta-content-desc{\n        display:flex;\n        text-align: center;\n    }\n}",
            "js":"let ctaimg = $scope[0].querySelectorAll(\".wkit-cta-image-box\");\n\nctaimg.forEach((e) => {\n    e.parentElement.style.width = '100%';\n});"
