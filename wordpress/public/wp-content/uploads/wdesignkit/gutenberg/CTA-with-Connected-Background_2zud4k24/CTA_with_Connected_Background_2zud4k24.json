            "css":".cta-with-bg-inner {\n    overflow: hidden;\n}\n.cta-with-bg-inner.layout-con-horizontal {\n    position: relative;\n    z-index: 1;\n    display: flex;\n    flex-wrap: wrap;\n    width: 100%;\n}\n.cta-with-bg-inner.layout-con-vertical {\n    position: relative;\n    z-index: 1;\n    display: block;\n}\n.con-image-background {\n    position: absolute;\n    z-index: -1;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n}\n.con-image-background [src=\"\"] {\n    display: none;\n}\n.con-image-background img.bg-image{\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    transition: all 0.4s ease 0s;\n    opacity: 0;\n    -o-object-fit: cover;\n    object-fit: cover;\n}\n.con-image-background img.bg-image.active {\n    opacity: 1;\n}\n.con-item-content [data-title=\"\"], .con-item-content [data-desc=\"\"], .con-item-content [data-btext=\"\"]{\n    display: none;\n}\n.con-image-item {\n    display: flex;\n    align-items: flex-end;\n    padding: 20px;\n    flex: 1 0;\n    overflow: hidden;\n}\n.cta-with-bg-inner .con-image-item {\n    --bdwidth : 1px;\n    --bdcolor : #fff;\n    --bgheight: 300px;\n    height: var(--bgheight);\n}\n.cta-with-bg-inner.layout-con-horizontal .con-image-item:not(:last-child){\n    border-right: var(--bdwidth) solid var(--bdcolor);\n}\n\n.cta-with-bg-inner.layout-con-vertical .con-image-item:not(:last-child){\n    border-bottom: var(--bdwidth) solid var(--bdcolor);\n}\n.con-image-item .con-item-content{\n    transition: all 0.4s ease 0s;\n    width: 100%;\n    --titleoffset : 25px;\n    transform: translateY(calc(100% - var(--titleoffset)));\n}\n.con-item-content .con-i-title {\n    font-size: 25px;\n    color: #000;\n}\n.con-item-content .con-i-desc {\n    font-size: 15px;\n    color: #000;\n    opacity: 0;\n    transition: all 0.4s ease 0s;\n}\n.con-image-item.active .con-item-content{\n    transform: translateY(0);\n}\n.con-item-content .con-i-btn{\n    font-size: 12px;\n    padding: 5px 15px;\n    opacity: 0;\n    color: #fff;\n    display: inline-flex;\n    align-items: center;\n    gap: 5px;\n    border-radius: 4px;\n    background-color: #000;\n    transition: all 0.4s ease 0s;\n    text-decoration: none;\n}\n.con-item-content .con-i-btn .tp-title-icon{\n    display: inline-flex;\n    align-items: center;\n    font-size: 12px;\n    color: #fff;\n}\n.con-item-content .con-i-btn .tp-title-icon i{\n    transition: all 0.4s ease 0s;\n}\n.con-item-content .tp-title-icon .dashicon{\n      font-size: 12px;\n    width: auto;\n    height: auto;\n}\n.con-image-item.active .con-item-content .con-i-desc, .con-image-item.active .con-item-content .con-i-btn {\n    opacity: 1;\n}\n\n@media (max-width: 767px){\n    .cta-with-bg-inner.layout-con-vertical, .cta-with-bg-inner.layout-con-horizontal.vertical-on-mobile-yes {\n        position: relative;\n        z-index: 1;\n        display: block;\n    }\n    .cta-with-bg-inner.layout-con-horizontal.vertical-on-mobile-yes .con-image-item:not(:last-child){\n        border-right: none !important;\n        border-bottom: var(--bdwidth) solid var(--bdcolor);\n    }\n    .con-image-item {\n        padding: 10px;\n    }\n    .con-item-content .con-i-btn {\n        padding: 5px 7px;\n    }\n}\n",
            "js":"let cBGAll = $scope[0].querySelector('.wkit-cta-with-con-bg');\nlet gtAllImg = cBGAll.querySelectorAll('.bg-image');\ngtAllImg.forEach((el,index)=>{\n    if(index==0){\n        el.classList.add('active');\n    }\n});\n\nlet gtAllItem = cBGAll.querySelectorAll('.con-image-item');\ngtAllItem.forEach((el,index)=>{\n    if(index==0){\n        el.classList.add('active');\n    }\n    \n    el.addEventListener('mouseenter', (e, index)=>{\n        let getbg = e.currentTarget.closest('.cta-with-bg-inner');\n        \n        let actImg = getbg.querySelector('.bg-image.active');\n        if(actImg){\n            actImg.classList.remove('active');\n        }\n        let actItem = getbg.querySelector('.con-image-item.active');\n        if(actItem){\n            actItem.classList.remove('active');\n        }\n        \n        e.currentTarget.classList.add('active');\n        \n        let gtIndex = e.currentTarget.getAttribute('data-index');\n        \n        let newImg = getbg.querySelector('img[data-index=\"'+gtIndex+'\"]');\n        newImg.classList.add('active');\n        \n        console.log(newImg);\n    });\n    \n});\n\n\n"
