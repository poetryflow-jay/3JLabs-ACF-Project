            "css":".wkit-cardslide-wrapper {\r\n  padding: 0 10px;\r\n}\r\n\r\n#tranding .wkit-slider {\r\n  display: flex;\r\n  flex-direction: column;\r\n  position: relative;\r\n  height: auto;\r\n  padding: 20px 0;\r\n  margin-bottom: 10px;\r\n}\r\n.wkit-slider .wkit-slide-img img[src=\"\"]{\r\n  display: none !important;\r\n}\r\n.wkit-cardslide-wrapper .wkit-slide {\r\n  position: relative;\r\n  width: 370px;\r\n  height: 420px;\r\n}\r\n.wkit-slide .wkit-slide-img{\r\n  height:100%;\r\n  width:100%;\r\n}\r\n.wkit-slide .wkit-slide-img img {\r\n    /*width: 370px;*/\r\n  /*height: 420px;*/\r\n  height:100%;\r\n  width:100%;\r\n  border-radius: 20px;\r\n}\r\n\r\n\r\n.wkit-cardslide-wrapper span.swiper-pagination-bullet.swiper-pagination-bullet {\r\n    margin: 5px;\r\n}\r\n\r\n\r\n/** Image Content */\r\n.wkit-slide .wkit-slide-content {\r\n  position: absolute;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  align-content: center;\r\n  left: 0;\r\n  top: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n\r\n.wkit-slide-content .wkit-slide-title {\r\n  font-size: 30px;\r\n  color: #fff;\r\n}\r\n\r\n.wkit-slide-content .wkit-slide-desc {\r\n  font-size: 18px;\r\n  color: #fff;\r\n  padding:0;\r\n  text-align:center;\r\n}\r\n\r\n.swiper-slide-shadow-left,\r\n.swiper-slide-shadow-right {\r\n  display: none;\r\n}\r\n\r\n/** Pagination CSS-Start */\r\n.wkit-slider-control {\r\n  position: relative;\r\n  bottom: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.wkit-slider-control .swiper-button-next {\r\n  left: 58% !important;\r\n  transform: translateX(-58%) !important;\r\n}\r\n\r\n@media (max-width:990px) {\r\n  .wkit-slider-control .swiper-button-next {\r\n    left: 70% !important;\r\n    transform: translateX(-70%) !important;\r\n  }\r\n\r\n  .wkit-slider-control .swiper-button-prev {\r\n    left: 30% !important;\r\n    transform: translateX(-30%) !important;\r\n  }\r\n}\r\n\r\n@media (max-width:500px) {\r\n  .wkit-cardslide-wrapper .wkit-slide {\r\n    width: 280px;\r\n    height: 360px;\r\n  }\r\n}\r\n\r\n@media (max-width:450px) {\r\n  .wkit-slider-control .swiper-button-next {\r\n    right: 0% !important;\r\n    left: auto !important;\r\n    transform: translateX(-17%) !important;\r\n  }\r\n\r\n  .wkit-slider-control .swiper-button-prev {\r\n    left: 5% !important;\r\n    transform: translateX(-5%) !important;\r\n  }\r\n}\r\n\r\n.wkit-slider-control .slider-arrow {\r\n  display:none;\r\n  background: #fff;\r\n  width: 35px;\r\n  height: 35px;\r\n  border-radius: 50%;\r\n  left: 42%;\r\n  transform: translateX(-42%);\r\n  filter: drop-shadow(0px 8px 24px rgba(18, 28, 53, 0.1));\r\n  transition: 0.3s linear;\r\n  z-index: 99;\r\n}\r\n.wkit-slider-control .slider-arrow .tp-title-icon {\r\n    transition: 0.3s linear;\r\n}\r\n.wkit-arrow-show{\r\n    display:flex !important;\r\n}\r\n.wkit-slider-control .slider-arrow ion-icon {\r\n  font-size: 20px;\r\n  color: #222224;\r\n  transition:0.3s linear;\r\n}\r\n\r\n.wkit-slider-control .slider-arrow::after {\r\n  content: '';\r\n}\r\n\r\n.wkit-slider-control .swiper-pagination {\r\n  display: none;\r\n  flex-direction: row;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n  bottom: 0;\r\n}\r\n.swiper-pagination-yes{\r\n    display:flex !important;\r\n}\r\n.wkit-slider-control .swiper-pagination .swiper-pagination-bullet {\r\n  filter: drop-shadow(0px 8px 24px rgba(18, 28, 53, 0.1));\r\n}\r\n\r\n.wkit-slider-control .swiper-pagination .swiper-pagination-bullet-active {\r\n  background: #ec994b;\r\n}",
            "js":"let getSlide = $scope[0].querySelector(\".wkit-cardslide-wrapper\");\r\nvar unid = Math.random().toString(32).slice(2);\r\nif(getSlide.getAttribute('data-unique')){\r\n    return;\r\n}else{\r\n    getSlide.setAttribute('data-unique', unid)\r\n}\r\nif(getSlide){\r\n    var unid = Math.random().toString(32).slice(2);\r\n    let swClss = getSlide.querySelector('.wkit-slider');\r\n    let swiperPag=getSlide.querySelector(\".swiper-pagination\");\r\n        swiperPag.classList.add('swiper-pag-'+unid);\r\n    let swiperbtnnext=getSlide.querySelector(\".swiper-button-next\");\r\n        swiperbtnnext.classList.add('swiper-nxt-'+unid);\r\n    let swiperbtnprev=getSlide.querySelector(\".swiper-button-prev\");\r\n        swiperbtnprev.classList.add('swiper-pre-'+unid);\r\n    var TrandingSlider = new Swiper(swClss, {\r\n        effect: 'coverflow',\r\n        grabCursor: true,\r\n        centeredSlides: true,\r\n        loop: true,\r\n        draggable:true,\r\n        slidesPerView: 'auto',\r\n        coverflowEffect: {\r\n            rotate: 0,\r\n            stretch: 0,\r\n            depth: 200,\r\n            modifier: 3,\r\n        },\r\n        pagination: {\r\n            el: '.swiper-pag-'+unid,\r\n            clickable: true,\r\n        },\r\n        navigation: {\r\n            nextEl: '.swiper-nxt-'+unid,\r\n            prevEl: '.swiper-pre-'+unid,\r\n        }\r\n    }); \r\n}\r\n\r\n"
