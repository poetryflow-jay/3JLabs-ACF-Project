            "css":".wkit-flip-card{\n    position:relative;\n    display:block;\n}\n.wkit-flip-card-inner{\n    width:100%;\n    height:100%;\n    position:relative;\n    display:flex;\n    justify-content:center; \n    align-items:center;\n}\n.wkit-flip-card-inner .wkit-flip-card-main{\n    position:relative;\n    width:100%;\n    height:150px;\n}\n.flip-card-frontside .flip-card-frontside-title{\n    font-size: 15px;\n    text-align: center;\n    color: #fff;\n}\n.flip-card-frontside .tp-title-icon{\n    font-size: 30px;\n    color: #fff;\n}\n.flip-card-frontside .tp-title-icon .dashicon{\n    width: auto;\n    height: auto;\n}\n.wkit-flip-card-main >.flip-card-frontside{\n    flex-direction:column-reverse;\n}\n.flip-card-backside .tp-title-icon{\n    position: relative;\n    text-align: center;\n    color: #fff;\n}\n.flip-card-backside .tp-title-icon .dashicon{\n    width: auto;\n    height: auto;\n}\n.flip-card-backside .flip-card-backside-content{\n    position: relative;\n    display: flex;\n    row-gap:5px;\n    flex-direction: column;\n}\n.flip-card-backside .flip-card-backside-content .flip-card-title{\n    font-size: 17px;\n    color: #ccc;\n}\n.flip-card-backside .flip-card-backside-content .flip-card-desk{\n    font-size: 15px;\n    color: #ccc;\n}\n.flip-card-backside .flip-card-backside-content .flip-card-link{\n    display:flex;\n    width:max-content;\n    justify-content:left;\n    align-items:center;\n    text-decoration: none;\n}\n.wkit-flip-card-main > .flip-card-frontside{\n   background-color: #000;\n   overflow: hidden;\n}\n.wkit-flip-card-main > .flip-card-backside{\n    background-color: #000;\n     padding: 10px;\n     overflow: hidden;\n}\n\n.wkit-flip-card-main > .flip-card-frontside , .wkit-flip-card-main > .flip-card-backside{\n   /*.wkit-flip-card-main > .wkit-card-1{*/\n    -webkit-backface-visibility: hidden;\n    -moz-backface-visibility: hidden;\n    -ms-backface-visibility: hidden;\n    -o-backface-visibility: hidden;\n    backface-visibility: hidden;\n    -webkit-transition: -webkit-transform 0.6s;\n    -moz-transition: -moz-transform 0.6s;\n    -o-transition: -o-transform 0.6s;\n    transition: transform 0.6s;\n    position: absolute;\n    height: 100%;\n    width: 100%;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n.wkit-flip-card-main > .flip-card-frontside{\n    -webkit-transform: perspective(1000) rotateY(0);\n    -moz-transform: perspective(1000) rotateY(0);\n    -ms-transform: perspective(1000) rotateY(0);\n    -o-transform: perspective(1000) rotateY(0);\n    transform: perspective(1000) rotateY(0);\n    z-index: 900;\n}\n.wkit-flip-card-main.type-horizontal:hover > .flip-card-frontside{\n     cursor: pointer;\n    transform: rotateY(180deg);\n    transform: perspective(1000) rotateY(180deg);\n}\n.wkit-flip-card-main.type-vertical:hover > .flip-card-frontside{\n    cursor: pointer;\n    transform: rotateX(180deg);\n    transform: perspective(1000) rotateX(180deg);\n\n}\n.wkit-flip-card-main.type-horizontal > .flip-card-backside{\n    transform: rotateY(-180deg);\n    z-index: 800;\n    flex-direction: row;\n    align-items: flex-start; \n}\n.wkit-flip-card-main.type-horizontal:hover > .flip-card-backside{\n    cursor: pointer;\n    z-index: 950;\n    transform: rotateY(0deg);\n    transform: perspective(1000) rotateY(0deg);\n}\n.wkit-flip-card-main.type-vertical > .flip-card-backside{\n    transform: rotateX(-180deg);\n    z-index: 800;\n    flex-direction: row;\n    align-items: flex-start; \n}\n.wkit-flip-card-main.type-vertical:hover > .flip-card-backside{\n    cursor: pointer;\n    z-index: 950;\n    transform: rotateX(0deg);\n    transform: perspective(1000) rotateX(0deg);\n}\n\n\n",
