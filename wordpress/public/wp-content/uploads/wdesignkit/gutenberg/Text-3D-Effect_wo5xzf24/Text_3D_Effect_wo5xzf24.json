            "css":".wkit-text-3d-effect{\r\n    display: block;\r\n    position: relative;\r\n    text-align: center;\r\n}\r\n.wkit-3d-stage {\r\n  position: relative;\r\n  display: inline-block;\r\n  perspective: 9999px;\r\n  transform-style: preserve-3d;\r\n}\r\n.w-3d-layer:first-child{\r\n    position: relative;\r\n}\r\n.w-3d-layer {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  --wkitTextRotateX: 33deg;\r\n  --wkitTextRotateY: 40deg;\r\n  transform-style: preserve-3d;\r\n  transition: 0.3s linear;\r\n  transform: rotateY(var(--wkitTextRotateY)) rotateX(var(--wkitTextRotateX)) translateZ(0);\r\n}\r\n.continuous-animation-yes .w-3d-layer{\r\n    animation: wkit-wave-effect 5s infinite alternate ease-in-out -7.5s;\r\n    animation-fill-mode: forwards;\r\n}\r\n.w-3d-layer span {\r\n  font-size: 45px;\r\n  text-align: center;\r\n  height: 100%;\r\n  width: 100%;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  color: whitesmoke;\r\n  letter-spacing: -2px;\r\n  text-shadow: 4px 0 10px rgba(0, 0, 0, 0.13);\r\n  transition: 0.3s linear;\r\n}\r\n\r\n.w-3d-layer:nth-child(1) span {\r\n  transform: translateZ(0px);\r\n}\r\n.w-3d-layer:nth-child(2) span {\r\n  transform: translateZ(-1.5px);\r\n}\r\n.w-3d-layer:nth-child(3) span {\r\n  transform: translateZ(-3px);\r\n}\r\n.w-3d-layer:nth-child(4) span {\r\n  transform: translateZ(-4.5px);\r\n}\r\n.w-3d-layer:nth-child(5) span {\r\n  transform: translateZ(-6px);\r\n}\r\n.w-3d-layer:nth-child(6) span {\r\n  transform: translateZ(-7.5px);\r\n}\r\n.w-3d-layer:nth-child(7) span {\r\n  transform: translateZ(-9px);\r\n}\r\n.w-3d-layer:nth-child(8) span {\r\n  transform: translateZ(-10.5px);\r\n}\r\n.w-3d-layer:nth-child(9) span {\r\n  transform: translateZ(-12px);\r\n}\r\n.w-3d-layer:nth-child(10) span {\r\n  transform: translateZ(-13.5px);\r\n}\r\n.w-3d-layer:nth-child(11) span {\r\n  transform: translateZ(-15px);\r\n}\r\n.w-3d-layer:nth-child(12) span {\r\n  transform: translateZ(-16.5px);\r\n}\r\n.w-3d-layer:nth-child(13) span {\r\n  transform: translateZ(-18px);\r\n}\r\n.w-3d-layer:nth-child(14) span {\r\n  transform: translateZ(-19.5px);\r\n}\r\n.w-3d-layer:nth-child(15) span {\r\n  transform: translateZ(-21px);\r\n}\r\n.w-3d-layer:nth-child(16) span {\r\n  transform: translateZ(-22.5px);\r\n}\r\n.w-3d-layer:nth-child(17) span {\r\n  transform: translateZ(-24px);\r\n}\r\n.w-3d-layer:nth-child(18) span {\r\n  transform: translateZ(-25.5px);\r\n}\r\n.w-3d-layer:nth-child(19) span {\r\n  transform: translateZ(-27px);\r\n}\r\n.w-3d-layer:nth-child(20) span {\r\n  transform: translateZ(-28.5px);\r\n}\r\n.w-3d-layer:nth-child(n+10) span {\r\n  -webkit-text-stroke: 3px rgba(0, 0, 0, 0.25);\r\n}\r\n.w-3d-layer:nth-child(n+11) span {\r\n  -webkit-text-stroke: 15px dodgerblue;\r\n  text-shadow: 6px 0 6px #00366b, 5px 5px 5px #002951, 0 6px 6px #00366b;\r\n}\r\n.w-3d-layer:nth-child(n+12) span {\r\n  -webkit-text-stroke: 15px #0077ea;\r\n}\r\n.w-3d-layer:last-child span {\r\n  -webkit-text-stroke: 17px rgba(0, 0, 0, 0.1);\r\n}\r\n.w-3d-layer:first-child span{\r\n  color: #fff;\r\n  text-shadow: none;\r\n  position: relative;\r\n}\r\n\r\n@keyframes wkit-wave-effect {\r\n  100% {\r\n    transform: rotateY(-40deg) rotateX(-43deg);\r\n  }\r\n}",
